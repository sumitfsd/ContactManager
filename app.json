{
  "expo": {
    "name": "ContactManager",
    "slug": "ContactManager",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "READ_CONTACTS"
      ]
    },
    "plugins": [
      [
        "expo-contacts",
        {
          "contactsPermission": "Allow ContactManger to access your contacts."
        }
      ]
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }  
}
